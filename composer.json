{
  "name": "programie/meal-plan",
  "description": "Webapp for managing meal plans",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "altorouter/altorouter": "^2.0",
    "guzzlehttp/guzzle": "^7.5",
    "twig/twig": "^3.5",
    "symfony/asset": "^6.2",
    "ext-json": "*",
    "symfony/yaml": "^6.2",
    "doctrine/orm": "^2.14",
    "symfony/cache": "^6.2",
    "twig/html-extra": "^3.5",
    "symfony/dependency-injection": "^6.2",
    "symfony/config": "^6.2",
    "symfony/console": "^6.2",
    "symfony/dotenv": "^6.2",
    "symfony/runtime": "^6.2",
    "symfony/framework-bundle": "^6.2",
    "symfony/flex": "^2.2",
    "symfony/twig-bundle": "^6.2",
    "twig/extra-bundle": "^2.12|^3.0",
    "doctrine/doctrine-bundle": "^2.9",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "symfony/translation": "^6.2"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "mealplan\\": "src/main/php/mealplan"
    }
  },
  "authors": [
    {
      "name": "Michael Wieland",
      "email": "git@selfcoders.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "symfony/flex": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
